{
  "start": {
    "tag": "label",
    "className": "cityLabel",
    "c": {
      "title": {
        "tag": "span",
        "textContent": "Please, enter a city name"
      },
      "cityInput": {
        "tag": "input",
        "type": "text",
        "className": "cityInput",
        "id": "cityInput"
      },
      "submitBtn": {
        "tag": "button",
        "id": "submitBtn",
        "textContent": "Ok"
      }
    }
  },

  "loader": {
    "tag": "div",
    "className": "loader"
  },

  "desktop": {
    "tag": "div",
    "className": "container",
    "c": {
      "cityLabel": {
        "tag": "label",
        "className": "cityLabel",
        "c": {
          "cityInput": {
            "tag": "input",
            "type": "text",
            "className": "cityInput",
            "id": "cityInput"
          },
          "submitBtn": {
            "tag": "button",
            "id": "submitBtn",
            "textContent": "Ok"
          }
        }
      },
      "cityName": {
        "tag": "span"
      },
      "current": {
        "tag": "div",
        "className": "currentTemp",
        "c": {
          "skyWrapper": {
            "tag": "div",
            "className": "sky",
            "c": {
              "sky": {
                "tag": "span",
                "id": "sky"
              },
              "skyImg": {
                "tag": "img",
                "id": "skyImg"
              }
            }
          },
          "tempWrapper": {
            "tag": "div",
            "className": "tempWrapper",
            "c": {
              "temperatures": {
                "tag": "div",
                "className": "temperatures",
                "c": {
                  "current": {
                    "tag": "span",
                    "className": "current",
                    "id": "current"
                  },
                  "feelsLike": {
                    "tag": "span",
                    "id": "feelsLike"
                  },
                  "minimal": {
                    "tag": "span",
                    "id": "minimal"
                  },
                  "maximal": {
                    "tag": "span",
                    "id": "maximal"
                  }
                }
              },
              "measure": {
                "tag": "span",
                "className": "measure",
                "innerHTML": "<sup>0</sup>C"
              }
            }
          },
          "currentDate": {
            "tag": "div",
            "className": "time",
            "c": {
              "date": {
                "tag": "time",
                "className": "date",
                "id": "date"
              },
              "time": {
                "tag": "time",
                "className": "time",
                "id": "time"
              }
            }
          },
          "otherData": {
            "tag": "div",
            "className": "otherData",
            "c": {
              "humidity": {
                "tag": "span",
                "id": "humidity"
              },
              "pressure": {
                "tag": "span",
                "id": "pressure",
                "textContent": "Pressure: "
              },
              "windSpeed": {
                "tag": "span",
                "id": "windSpeed"
              },
              "deg": {
                "tag": "span",
                "id": "deg"
              }
            }
          },
          "sun": {
            "tag": "div",
            "className": "sun",
            "c": {
              "sunrise": {
                "tag": "div",
                "className": "sunrise",
                "c": {
                  "name": {
                    "tag": "h4",
                    "textContent": "Sunrise"
                  },
                  "time": {
                    "tag": "time",
                    "id": "sunrise"
                  }
                }
              },
              "sunset": {
                "tag": "div",
                "className": "sunset",
                "c": {
                  "name": {
                    "tag": "h4",
                    "textContent": "Sunset"
                  },
                  "time": {
                    "tag": "time",
                    "id": "sunset"
                  }
                }
              }
            }
          }
        }
      },
      "forecasts": {
        "tag": "div",
        "c": {
          "i": {
            "hourly": "hourlyCard",
            "daily": "dailyCard"
          }
        }
      }
    }
  }
}
