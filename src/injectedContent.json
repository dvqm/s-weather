{
  "hourlyForecast": {
    "tag": "div",
    "className": "hourly dispRow",
    "c": {
      "previous": {
        "tag": "button",
        "id": "hourlyPrev",
        "className": "prevBtn"
      },
      "wrapper": {
        "tag": "div",
        "className": "cards",
        "id": "hourlyCards",
        "c": {
          "hourlyCard": {
            "tag": "div",
            "className": "card",
            "c": {
              "hour": {
                "tag": "time"
              },
              "icon": {
                "tag": "img"
              },
              "descr": {
                "tag": "span"
              },
              "temperature": {
                "tag": "span",
                "s": "<sup>0</sup>C"
              },
              "pressure": {
                "tag": "span"
              },
              "humidity": {
                "tag": "span"
              },
              "windSpeed": {
                "tag": "span"
              },
              "deg": {
                "tag": "span"
              }
            }
          }
        }
      },
      "next": {
        "tag": "button",
        "id": "hourlyNext",
        "className": "nextBtn"
      }
    }
  },
  "dailyForecast": {
    "tag": "div",
    "className": "daily dispRow",
    "c": {
      "previous": {
        "tag": "button",
        "id": "dailyPrev",
        "className": "prevBtn"
      },
      "wrapper": {
        "tag": "div",
        "className": "cards",
        "id": "dailyCards",
        "c": {
          "dailyCard": {
            "tag": "div",
            "className": "card",
            "c": {
              "date": {
                "tag": "time"
              },
              "icon": {
                "tag": "img"
              },
              "descr": {
                "tag": "span"
              },
              "sun": {
                "tag": "div",
                "className": "sun",
                "c": {
                  "sunrise": {
                    "tag": "time"
                  },
                  "sunset": {
                    "tag": "time"
                  }
                }
              },
              "temperatures": {
                "tag": "div",
                "className": "predict",
                "c": {
                  "morning": {
                    "tag": "span"
                  },
                  "day": {
                    "tag": "span"
                  },
                  "evening": {
                    "tag": "span"
                  },
                  "night": {
                    "tag": "span"
                  }
                }
              },
              "otherData": {
                "tag": "div",
                "className": "other",
                "c": {
                  "pressure": {
                    "tag": "span"
                  },
                  "humidity": {
                    "tag": "span"
                  },
                  "windSpeed": {
                    "tag": "span"
                  },
                  "deg": {
                    "tag": "span"
                  },
                  "uvi": {
                    "tag": "span"
                  }
                }
              }
            }
          }
        }
      },
      "next": {
        "tag": "button",
        "id": "dailyNext",
        "className": "nextBtn"
      }
    }
  }
}
